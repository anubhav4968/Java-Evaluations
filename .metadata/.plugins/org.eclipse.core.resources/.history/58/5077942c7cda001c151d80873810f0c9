package com.masai;

public class Shapes {

	public void areaCircle(int circle) {
		int area = (4/3)*(22/7)*(circle*circle);
		System.out.println("Area Of A circle: "+area);
	}
	
	public void areaReatangle(int length, int breath) {
		int area = 2*(length+breath);
		System.out.println("Area Of A Reactangle: "+area);
	}
	
	public void areaSquare(int side) {
		int area = side*side;
		System.out.println("Area Of A Square: "+area);
	}
}
